{
  "name": "highstation",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "bash scripts/ops/sync-env.sh && ./node_modules/.bin/tsc && cd dashboard && npm install --include=dev && npm run build",
    "build:backend": "./node_modules/.bin/tsc",
    "build:frontend": "bash scripts/ops/sync-env.sh && cd dashboard && npm run build",
    "vercel-build": "npm run build",
    "start": "node dist/src/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "dev:frontend": "cd dashboard && npm run dev",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:frontend\" --names \"SERVER,CLIENT\" --prefix-colors \"blue,magenta\"",
    "dev:local": "bash scripts/ops/dev-start.sh",
    "stop:dev": "bash scripts/ops/dev-stop.sh",
    "deploy:cronos": "node dist/scripts/ops/deploy-cronos.js",
    "verify:cronos": "node dist/scripts/ops/simulate_cronos.js",
    "demo": "node dist/scripts/ops/run_demo.js",
    "request": "bash scripts/test/request.sh",
    "test:node": "anvil --fork-url https://evm-t3.cronos.org --chain-id 338 --port 8545",
    "test": "npx jest",
    "test:watch": "npx jest --watch",
    "test:coverage": "npx jest --coverage",
    "lint": "npx eslint . --ext .ts",
    "lint:fix": "npx eslint . --ext .ts --fix",
    "format": "npx prettier --write \"src/**/*.ts\" \"tests/**/*.ts\" \"scripts/**/*.ts\"",
    "format:check": "npx prettier --check \"src/**/*.ts\" \"tests/**/*.ts\" \"scripts/**/*.ts\"",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/jest": "^30.0.0",
    "@types/morgan": "^1.9.10",
    "@types/node": "^25.0.3",
    "@types/pg": "^8.16.0",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.52.0",
    "@typescript-eslint/parser": "^8.52.0",
    "axios": "^1.13.2",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "supertest": "^7.2.2",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@coinbase/x402": "^2.1.0",
    "@crypto.com/ai-agent-client": "^1.0.2",
    "@crypto.com/developer-platform-client": "^1.1.2",
    "@crypto.com/facilitator-client": "^1.0.2",
    "@modelcontextprotocol/sdk": "^1.25.2",
    "@noble/ed25519": "^3.0.0",
    "@noble/hashes": "^2.0.1",
    "@openzeppelin/contracts": "^5.4.0",
    "@pythnetwork/hermes-client": "^2.0.0",
    "@supabase/supabase-js": "^2.90.0",
    "@types/node-cron": "^3.0.11",
    "@types/sqlite3": "^3.1.11",
    "circomlib": "^2.0.5",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "morgan": "^1.10.1",
    "node-cron": "^4.2.1",
    "permissionless": "^0.3.2",
    "pg": "^8.16.3",
    "snarkjs": "^0.7.6",
    "viem": "^2.43.5",
    "zod": "^4.3.5"
  }
}